{
	"name": "eventos-backend",
	"version": "3.0.0",
	"private": true,
	"type": "module",
	"engines": {
		"node": ">=20.0.0"
	},
	"scripts": {
		"dev": "tsx watch src/server.ts",
		"build": "tsc -p tsconfig.json",
		"start": "node dist/server.js",
		"lint:types": "tsc --noEmit -p tsconfig.json",
		"lint": "eslint -c ./eslint.config.js \"src/**/*.{ts,tsx}\"",
		"lint:fix": "eslint -c ./eslint.config.js \"src/**/*.{ts,tsx}\" --fix",
		"lint:cache": "eslint -c ./eslint.config.js \"src/**/*.{ts,tsx}\" --cache --cache-location .eslintcache --cache-strategy content",
		"check:lint": "npm run lint:types && npm run lint",
		"check:mongo": "tsx src/scripts/check-mongo.ts",
		"check:imports": "tsx src/scripts/check-import-extensions.ts",
		"seed": "tsx src/scripts/seed.ts",
		"format": "prettier -w ."
	},
	"dependencies": {
		"@asteasolutions/zod-to-openapi": "7.3.4",
		"@fastify/cors": "^10.0.0",
		"@fastify/rate-limit": "^10.3.0",
		"@fastify/swagger": "9.5.2",
		"@fastify/swagger-ui": "5.2.3",
		"dotenv": "^16.4.5",
		"fastify": "^5.6.1",
		"fastify-plugin": "^5.0.0",
		"fastify-type-provider-zod": "^4.0.2",
		"mongodb": "^6.20.0",
		"openapi-types": "12.1.3",
		"pino": "^9.3.2",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3.3.1",
		"@types/node": "^22.5.4",
		"@typescript-eslint/eslint-plugin": "^8.0.0",
		"@typescript-eslint/parser": "^8.46.1",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-import": "^2.32.0",
		"eslint-plugin-unicorn": "^55.0.0",
		"pino-pretty": "^13.1.1",
		"prettier": "^3.6.2",
		"tsx": "^4.19.1",
		"typescript": "^5.6.2"
	}
}
